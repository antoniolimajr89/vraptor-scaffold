<body>

<% @attributes.each do |att| -%>
<p>
  <b><%= att.html_label %>:</b>
   <%= "${#{model_parameter_name}.#{att.name}!\"\"}" unless att.boolean? %>
   <%= "${#{model_parameter_name}.#{att.name}?string}" if att.boolean? %>
</p>
<% end -%>

<a href="<%= "#{path}/${#{model_parameter_name}.id}/edit" %>">Edit</a>
<a href="<%= path %>">Back</a>

</body>
